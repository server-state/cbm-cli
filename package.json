	"license": "MIT",
	"bugs": {
		"url": "https://github.com/server-state/cli/issues"
	},
	"homepage": "https://github.com/server-state/cli#readme",
	"devDependencies": {
		"@types/yargs": "^15.0.5",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "^6.11.0",
		"prettier": "^2.0.5"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"clui": "^0.3.6",
		"debug": "^4.1.1",
		"extract-zip": "^2.0.1",
		"inquirer": "^7.3.3",
		"supports-color": "^7.1.0",
		"tmp": "^0.2.1",
