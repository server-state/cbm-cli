{
	"name": "@server-state/cli",
	"version": "0.1.0",
	"description": "The Server State command line interface",
	"bin": {
		"server-state": "./bin/cli.js"
	},
	"scripts": {
		"submodule:init": "git submodule init",
		"submodule:update": "git submodule update",
		"prettier": "prettier --write package.json \"bin/**/*\" \"src/**/*\"",
		"lint": "eslint --cache --ext .js .eslintrc.js ./bin ./src",
